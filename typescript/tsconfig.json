{
    "$schema": "https://json.schemastore.org/tsconfig",
    "_version": "20.1.0",
    "compilerOptions": {
        "lib": [
            "es2023"
        ],
        "types": [
            "node"
        ],
        "module": "node16",
        "target": "es2022",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "moduleResolution": "Node16"
    },
}